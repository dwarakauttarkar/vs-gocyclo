{
  "name": "vs-gocyclo",
  "displayName": "vs-gocyclo",
  "description": "vs extension for go code insights",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gocyclo.helloWorld",
        "title": "GoCyclo: Hello World"
      },
      {
        "command": "gocyclo.runGoCycle",
        "title": "GoCyclo: Run Go Cyclo"
      },
      {
        "command": "gocyclo.toggleStatus",
        "title": "GoCyclo: Toggle Show/Hide Complexity Bar"
      },
      {
        "command": "gocyclo.getTotalComplexity",
        "title": "GoCyclo: Get Total Complexity"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "string-table": "^0.1.5"
  }
}
